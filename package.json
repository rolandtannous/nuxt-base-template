{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:dev": "NUXT_VITEST_DEV_TEST=true nuxt dev --no-fork",
    "test:unit": "vitest",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:happy-dom": "VITEST_DOM_ENV=happy-dom npm run test:unit -- --run",
    "test": "npm run test:happy-dom && npm run test:dev && npm run test:types",
    "lint": "eslint . --ext .vue,.ts,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@nuxt/ui": "^2.15.2",
    "@pinia/nuxt": "^0.5.1",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.12.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@typescript-eslint/parser": "^6.21.0",
    "@vue/test-utils": "^2.4.5",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "happy-dom": "^14.7.1",
    "msw": "^2.2.14",
    "nuxt-lodash": "^2.5.3",
    "nuxt-schema-org": "^3.3.6",
    "playwright-core": "^1.43.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  }
}
