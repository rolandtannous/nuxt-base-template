{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:dev": "NUXT_VITEST_DEV_TEST=true nuxt dev --no-fork",
    "test:unit": "vitest",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:happy-dom": "VITEST_DOM_ENV=happy-dom npm run test:unit -- --run",
    "test": "npm run test:happy-dom && npm run test:dev && npm run test:types",
    "lint": "eslint . --ext .vue,.ts,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@nuxt/ui": "^2.21.0",
    "@pinia/nuxt": "^0.9.0",
    "nuxt": "^3.15.4",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.15.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@typescript-eslint/parser": "^8.23.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.5.0",
    "@vueuse/nuxt": "^12.5.0",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "happy-dom": "^16.8.1",
    "msw": "^2.7.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-schema-org": "^4.1.1",
    "playwright-core": "^1.50.1",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  }
}
